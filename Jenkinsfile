pipeline {
	agent any 
	stages {
		stage ('SCM initialize')
		{
			steps {
			echo "Hello World"
			}
		}
		stage ('Maven validate')
		{
			steps {
			echo "Maven validate"
			}
		}
		stage ('Maven compile')
		{
			steps {
			echo "Maven compile"
			}
		}
		stage ('Maven test')
		{
			steps {
			echo "Maven test"
			}
		}
		stage ('Maven verify')
		{
			steps {
			echo "Maven verify"
			}
		}
	}
}
